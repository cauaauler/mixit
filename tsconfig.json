	"compilerOptions": {
		"module": "ESNext",
		"target": "ES6",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"rootDir": "./backend",
		"outDir": "./dist",
		"allowImportingTsExtensions": true,
		"noEmit": true,
